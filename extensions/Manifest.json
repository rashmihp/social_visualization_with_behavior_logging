{
  "manifest_version": 2,
  "name": "Let's code",
  "version": "1.0",
"content_scripts": [
    {
        "matches": [
          "https://stackoverflow.com/*"
        ],
        "js":["jquery-3.3.1.min.js","content.js"],
        "run_at": "document_end"
    }
],
"browser_action": {
  "default_icon": "icon.png",
  "default_popup": "popup.html",
  "default_title": "wow popup"

},
"permissions": [
  "http://localhost:3000/*",
  "https://localhost:3000/*"
]

}
